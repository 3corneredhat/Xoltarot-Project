<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0"
	stylesheets="@../xoltarot.css" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="xoltarot.controller.CardSearchController">
	<children>
		<Label alignment="CENTER" layoutX="22.0" layoutY="14.0"
			prefHeight="100.0" prefWidth="963.0" stylesheets="@../xoltarot.css"
			text="Card Search">
			<font>
				<Font name="Bell MT" size="36.0" />
			</font>
		</Label>
		<Button layoutX="53.0" layoutY="670.0" mnemonicParsing="false"
			onAction="#Home" text="Home">
			<padding>
				<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
			</padding>
		</Button>
		<ListView fx:id="cards" layoutX="350.0" layoutY="167.0"
			prefHeight="416.0" prefWidth="300.0" />
		<TextField fx:id="searchBar" layoutX="351.0" layoutY="116.0"
			prefHeight="22.0" prefWidth="212.0" />
		<Button fx:id="searchButton" layoutX="584.0" layoutY="116.0"
			mnemonicParsing="false" onAction="#search" prefHeight="22.0"
			prefWidth="66.0" text="Search" />
		<Button layoutX="686.0" layoutY="333.0" mnemonicParsing="false"
			onAction="#display" prefHeight="84.0" prefWidth="87.0" text="Display" />
	</children>
</AnchorPane>
