<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0"
	stylesheets="@../xoltarot.css" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="xoltarot.controller.CardSearchController">
	<children>
		<AnchorPane layoutX="19.0" layoutY="127.0"
			prefHeight="610.0" prefWidth="963.0"
			style="-fx-background-color: #7F3E98; -fx-background-radius: 10;">
			<children>
				<Button layoutX="656.0" layoutY="265.0"
					mnemonicParsing="false" onAction="#display" prefHeight="80.0"
					prefWidth="200.0" text="Display Card Description">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
					<font>
						<Font size="16.0" />
					</font>
				</Button>
			</children>
		</AnchorPane>
		<Label alignment="CENTER" layoutX="22.0" layoutY="14.0"
			prefHeight="100.0" prefWidth="963.0" text="Card Search">
			<font>
				<Font name="Bell MT" size="36.0" />
			</font>
		</Label>
		<Button layoutX="53.0" layoutY="670.0" mnemonicParsing="false"
			onAction="#Home" text="Home">
			<padding>
				<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
			</padding>
		</Button>
		<ListView fx:id="cards" layoutX="350.0" layoutY="226.0"
			prefHeight="491.0" prefWidth="300.0" />
		<TextField fx:id="searchBar" layoutX="350.0" layoutY="157.0"
			prefHeight="28.0" prefWidth="229.0" />
		<Button fx:id="searchButton" layoutX="584.0" layoutY="157.0"
			mnemonicParsing="false" onAction="#search" prefHeight="22.0"
			prefWidth="66.0" text="Search" />
	</children>
</AnchorPane>
