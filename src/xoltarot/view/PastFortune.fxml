<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="750.0" prefWidth="1000.0"
	stylesheets="@../xoltarot.css" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="xoltarot.controller.PastFortuneController">
	<children>
		<AnchorPane layoutX="19.0" layoutY="127.0"
			prefHeight="610.0" prefWidth="963.0"
			style="-fx-background-color: #7F3E98; -fx-background-radius: 10;">
			<children>
				<TextArea fx:id="pastFortunes" layoutX="139.0"
					layoutY="49.0" prefHeight="471.0" prefWidth="686.0" />
				<Button fx:id="showPastFortunes" layoutX="408.0"
					layoutY="558.0" mnemonicParsing="false" onAction="#showPastFort"
					text="Show Past Fortunes ">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
				</Button>
			</children>
		</AnchorPane>
		<Label alignment="CENTER" layoutX="22.0" layoutY="14.0"
			prefHeight="100.0" prefWidth="963.0" stylesheets="@../xoltarot.css"
			text="Past Fortunes">
			<font>
				<Font name="Bell MT" size="36.0" />
			</font>
		</Label>
		<Button layoutX="53.0" layoutY="670.0" mnemonicParsing="false"
			onAction="#Home" text="Home">
			<padding>
				<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
			</padding>
		</Button>
	</children>
</AnchorPane>
